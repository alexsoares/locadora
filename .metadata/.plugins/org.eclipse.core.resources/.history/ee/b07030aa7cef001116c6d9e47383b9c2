package RecursosHumanos;

public abstract class Pessoa {

	private String nome;
	private String rua;
	private String bairro;
	private int numero;
	private String cidade;
	private int telefone;
	private String email;

	public Pessoa(String nome, String rua, String bairro, int numero, String cidade, int telefone, String email){
		this.nome = nome;
		this.rua = rua;
		this.bairro = bairro;
		this.numero = numero;
		this.cidade = cidade;
		this.telefone = telefone;
		this.email = email;
	}
	
    public String qualseunome(){
    	return this.nome;
    }
	
}
